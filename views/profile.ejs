<%- include('header') -%>
    <div class="jumbotron">
        <h1>Profile for <%= user.userName %></h1>
        <img src="<%= user.profilePicURL%>" width="200">
        <hr>
        <p> Welcome <%= user.googlename %>
            with email <%= user.googleemail %>
            You are logged in.<br><br>

            Nickname: <%= user.userName %> <br>
            Age: <%= user.age %><br>
            Gender:<%= user.gender %>
        <p> zipcode: <%= user.zipcode %> <br>
           location:<%= user.city %> <%= user.state %>
        </p>
        <p>Last updated on <%= user.lastUpdate %> </p>
        <a href="/editProfile">edit profile</a>
    </div>

<%- include('footer') -%>
